version: 1.1
nodes:
  $1 = store
  tryLogin = ellx.login()
  docs = store.documents
  getAuth = tryLogin && withStatus("auth")
  auth = tryLogin && ellx.auth
  authStatus = tryLogin && getAuth()
  profile = store.profile
  profileReady = profile.owner === userId
  userId = !authError ? (auth ? auth.userId : null) : null
  appId = !authError ? auth.appId : null
  regStep = route.includes("signup") ? +(route.split('/')[2] || 1) : null
  routeUserId = route.split('/')[2]
  chartDocId = routeUrl.searchParams.get("chart_doc_id")
  activeDocChartData = chartData(chartDocId, docs)
  chartDoc = withStatus("activeDocChartData")
  docId = route.split('/')[3]
  chartDocStatus = chartDoc()
  isAuthenticated = Boolean(userId)
  isAnon = docId && (!userId || userId !== routeUserId)
  $4 = isAnon && getDoc(docId)
  activeSheet = store.documents.get(docId)
  unsubscribeDocs = connect(userId)
  routeUrl = new URL("http://internet.com" + router.path)
  unsubscribeProfile = connectProfile(userId)
  route = routeUrl.pathname
  sheetChanged = activeSheet.owner && docId
  router = ellx.router
  title = activeSheet.title
  founderShare = calcFounderShare(activeSheet)
  groups = uniqueGroups(investors)
  investorGroups = groupInvestors(groups, investors)
  investors = activeSheet.investors
  rounds = activeSheet.rounds
  calculated = calculate(rounds, investors)
  plans = store.plans
